!!! html
%html{ html_attrs('ru')}
  %head
    %title #{title}
    = csrf_meta_tag
    = render 'layouts/stylesheets'
  %body
    #page
      = render 'layouts/header'
      %section
        #left
          = render 'layouts/nav'
        #center
          = yield
      = render 'layouts/footer'
      = debug(params) if Rails.env.development?